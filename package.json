{
  "name": "atom-focus-mode",
  "main": "./lib/focus-mode",
  "version": "0.1.2",
  "description": "Fades editor content and highlights only the lines you are working on.",
  "activationCommands": {
    "atom-workspace": [
      "focus-mode:toggle",
      "focus-mode:toggle-single-line",
      "focus-mode:toggle-shadow-mode"
    ]
